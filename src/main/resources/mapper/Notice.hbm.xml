<?xml  version="1.0" encoding="UTF-8"  ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.whpu.k160345.entity">
    <class name="com.whpu.k160345.entity.Notice" table="notice">
        <id name="id" column="id" type="java.lang.Integer" length="4">
            <generator class="native" />
        </id>
        <property name="title" column="title" type="java.lang.String" length="100" />
        <property name="content" column="content" type="java.lang.String" length="2000" />
        <property name="dateTimes" column="dateTimes" type="java.sql.Date" length="50" />
    </class>
</hibernate-mapping>